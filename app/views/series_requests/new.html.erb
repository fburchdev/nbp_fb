<p>Interested in finding the correct reading order of a series that you don't see? Request it here:</p>

<% flash.each do |key, value| %>
    <div class="<%= flash_class(key) %>">
        <%= value %>
    </div>
<% end %>

<%= form_for @series_request do |f| %>
  <%=f.label :email %>
  <%=f.email_field :email %>
  <%=f.label :series %>
  <%=f.text_field :series %>
  <%=f.label :author %>
  <%=f.text_field :author %>
  <%=f.submit 'Request A Series' %>
<% end %>
<br/>
